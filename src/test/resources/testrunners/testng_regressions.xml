<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="STOTE Test Regression PW Suite" thread-count="2"
	parallel="tests" verbose="4">

	<listeners>
		<listener
			class-name="com.listeners.ExtentReportListener" />

	</listeners>
	<test name="Open Cart Home Page Test_chrome">
		<!-- <parameter name="browser" value="chrome" /> -->
		<classes>
			 <class name="com.web.tests.HomePageTest" /> 
		</classes>
	</test>

	<test name="Open Cart Login Page Test_chrome">
		<!-- <parameter name="browser" value="chrome" /> -->
		<classes>
			<class name="com.web.tests.LoginPageTest" /> 
			<!-- <class name="com.web.tests.DashboardPageTest" />  -->
		</classes>
	</test>

</suite>